<table class ="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% if @courses != nil %>
      <% @courses.each do |c| %>
        <tr>
          <div class="container-fluid">
            <td><%= c.name %></td>
          </div>
        </tr>
        <% end %>
      </tbody>
    <% else %>
      <p> No Results </p>
    <%end%>
</table>
