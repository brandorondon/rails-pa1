<h1> Instructors </h1>
<table class ="table table-hover">
  <thead>
    <tr>
      <th>Name</th>

      <th>Email</th>

    </tr>
  </thead>


  <tbody>
    <% @instructors.each do |inst| %>
      <tr>
        <div class="container-fluid">
          <td><%= inst.last%>, <%= inst.first %></td>
          <td><%= inst.email %>
        </div>
      </tr>
    <% end %>
  </tbody>
</table>
